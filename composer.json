{
	"name": "quadlayers/storefront-footer",
	"description": "Storefront Footer",
	"type": "project",
	"authors": [
		{
			"name": "QuadLayers",
			"email": "noreply@quadlayers.com"
		}
	],
	"minimum-stability": "stable",
	"config": {
		"platform": {
			"php": "7.4.0"
		},
		"allow-plugins": {
			"quadlayers/wp-autoload": true,
			"automattic/jetpack-composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"php": ">=7.4.0",
		"automattic/jetpack-composer-plugin": "*",
		"automattic/jetpack-assets": "*",
		"quadlayers/wp-dashboard-widget-news": "*",
		"quadlayers/wp-notice-plugin-required": "*",
		"quadlayers/wp-notice-plugin-promote": "*",
		"quadlayers/wp-plugin-table-links": "*",
		"quadlayers/wp-plugin-suggestions": "*",
		"quadlayers/wp-plugin-install-tab": "*",
		"franmastromarino/wp-plugin-feedback": "*"
	},
	"autoload": {
		"classmap": [
			"lib/"
		]
	},
	"autoload-dev": {
		"exclude-from-classmap": [
			"lib/"
		]
	},
	"extra": {
		"quadlayers/wp-autoload": {
			"QuadLayers\\STFT": "lib/"
		},
		"wp-plugin-slug": "storefront-footer"
	},
	"require-dev": {
		"quadlayers/wp-autoload": "*"
	},
	"scripts": {}
}
